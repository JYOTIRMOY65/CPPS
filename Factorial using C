//Program Name: Find factorial of a number using function.
//Date: 26/09/2024
#include<stdio.h>
fact(int);
main()
{
    int i,factorial,num;
    printf("Enter a number: ");
    scanf("%d",&num);
    factorial = fact(num);
    printf("Factorial of %d is: %d",num,factorial);
    
}
int fact(int num)
{
    int i,f=1;
    for(i=1;i<=num;i++)
    f=f*i;
}
